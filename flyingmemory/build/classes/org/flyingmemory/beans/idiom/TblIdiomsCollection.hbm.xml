<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.flyingmemory.beans.idiom">
	<class name="TblIdiomsCollection" table="TBL_IDIOMS_COLLECTION">
		<id 
			name="idiomId"
			type="string"
			column="IDIOM_ID"
		>
			<generator class="assigned" />
		</id>
		
		<property name="idiom" column="IDIOM" type="string" not-null="true" length="32" />
		<property name="idiomInitials" column="IDIOM_INITIALS" type="string" not-null="true" length="4" />
		<property name="idiomDesc" column="IDIOM_DESC" type="string" not-null="false" length="256" />
		<property name="idiomStatus" column="IDIOM_STATUS" type="string" not-null="true" length="1" />
		<property name="instDate" column="INST_DATE" type="string" not-null="false" length="8" />
		<property name="instTime" column="INST_TIME" type="string" not-null="false" length="6" />

	</class>
</hibernate-mapping>
